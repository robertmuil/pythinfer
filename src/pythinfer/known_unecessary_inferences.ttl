@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

owl:Nothing
    a owl:Class ;
    rdfs:subClassOf
        owl:Nothing ,
        owl:Thing ;
    owl:equivalentClass owl:Nothing ;
.

owl:Thing
    a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    owl:equivalentClass owl:Thing ;
.