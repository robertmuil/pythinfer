@prefix : <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<basic-data.ttl> {
    :Bob a foaf:Person ;
        foaf:knows :Alice ;
        foaf:name "Bob Jones" .

    :Alice a foaf:Person ;
        foaf:age 30 ;
        foaf:name "Alice Smith" .
}

<basic-model.ttl> {
    foaf:knows a owl:SymmetricProperty .
}

