@prefix ex: <http://example.org/ancestor/> .

<inferences_owl> {
    ex:David ex:childOf ex:Bob ;
        ex:descendantOf ex:Alice,
            ex:Bob .

    ex:Eve ex:childOf ex:Carol ;
        ex:descendantOf ex:Alice,
            ex:Carol .

    ex:Bob ex:ancestorOf ex:David ;
        ex:childOf ex:Alice ;
        ex:descendantOf ex:Alice .

    ex:Carol ex:ancestorOf ex:Eve ;
        ex:childOf ex:Alice ;
        ex:descendantOf ex:Alice .

    ex:Alice ex:ancestorOf ex:Bob,
            ex:Carol,
            ex:David,
            ex:Eve .

    ex:Alice ex:grandparentOf ex:David,
            ex:Eve .
}
