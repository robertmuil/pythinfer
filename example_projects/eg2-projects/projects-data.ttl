@prefix ptp: <http://example.org/pythinfer/project/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

[] owl:imports <http://example.org/pythinfer/project/ontology> .

# ===== EXAMPLE INSTANCES =====

ptp:ExampleProject a ptp:Project ;
    dct:title "Example Pythinfer Project"@en ;
    dct:description "A sample project demonstrating the ontology structure."@en ;
    dct:creator "Project Creator" ;
    dct:created "2025-11-26"^^xsd:date ;
    ptp:basePath "/path/to/project" ;
    ptp:hasInternalData [
        a ptp:DataFile ;
        ptp:inputPattern "data/*.ttl" ;
        dct:title "Project Data Files"
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/skos.ttl" ;
        dct:title "SKOS Reference Vocabulary" ;
        dct:isVersionOf <http://www.w3.org/2004/02/skos/core>
    ] ;
    ptp:hasInferenceRule [
        a ptp:OwlRuleSet ;
        dct:title "OWL-RL Inference"@en ;
        ptp:inferenceOrder 1
    ] ;
    ptp:hasOutputConfiguration [
        a ptp:OutputConfiguration ;
        ptp:outputFolder "derived/"
    ] ;
    ptp:usesBackend [
        a ptp:ProcessingBackend ;
        ptp:backendName "rdflib" ;
        ptp:backendVersion "7.0.0"
    ] .

ptp:TaxonomyProject a ptp:Project ;
    dct:title "Taxonomy Classification Project"@en ;
    dct:description "A project for managing and classifying taxonomic hierarchies using shared vocabularies."@en ;
    dct:creator "Taxonomy Team" ;
    dct:created "2025-11-20"^^xsd:date ;
    ptp:basePath "/path/to/taxonomy" ;
    ptp:hasInternalData [
        a ptp:DataFile ;
        ptp:inputPattern "taxonomy-data/*.ttl" ;
        dct:title "Taxonomic Classifications"
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/skos.ttl" ;
        dct:title "SKOS Reference Vocabulary" ;
        dct:isVersionOf <http://www.w3.org/2004/02/skos/core>
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/rdfs.ttl" ;
        dct:title "RDFS Vocabulary" ;
        dct:isVersionOf <http://www.w3.org/2000/01/rdf-schema#>
    ] ;
    ptp:hasInferenceRule [
        a ptp:OwlRuleSet ;
        dct:title "OWL-RL Inference"@en ;
        ptp:inferenceOrder 1
    ] ;
    ptp:hasInferenceRule [
        a ptp:SparqlRule ;
        dct:title "Hierarchy Inference"@en ;
        ptp:inferenceOrder 2
    ] ;
    ptp:hasOutputConfiguration [
        a ptp:OutputConfiguration ;
        ptp:outputFolder "derived/"
    ] ;
    ptp:usesBackend [
        a ptp:ProcessingBackend ;
        ptp:backendName "rdflib" ;
        ptp:backendVersion "7.0.0"
    ] .

ptp:UnrelatedProject a ptp:Project ;
    dct:title "Unrelated Knowledge Graph Project"@en ;
    dct:description "A project using different vocabularies, with no shared external sources with other projects."@en ;
    dct:creator "Knowledge Graph Team" ;
    dct:created "2025-11-15"^^xsd:date ;
    ptp:basePath "/path/to/knowledge-graph" ;
    ptp:hasInternalData [
        a ptp:DataFile ;
        ptp:inputPattern "kg-data/*.ttl" ;
        dct:title "Knowledge Graph Assertions"
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/schema-org.ttl" ;
        dct:title "Schema.org Vocabulary" ;
        dct:isVersionOf <https://schema.org/>
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/foaf.ttl" ;
        dct:title "FOAF Vocabulary" ;
        dct:isVersionOf <http://xmlns.com/foaf/0.1/>
    ] ;
    ptp:hasInferenceRule [
        a ptp:PythonRule ;
        dct:title "Custom Entity Linking"@en ;
        ptp:inferenceOrder 1
    ] ;
    ptp:hasOutputConfiguration [
        a ptp:OutputConfiguration ;
        ptp:outputFolder "derived/"
    ] ;
    ptp:usesBackend [
        a ptp:ProcessingBackend ;
        ptp:backendName "pyoxigraph" ;
        ptp:backendVersion "0.3.0"
    ] .
