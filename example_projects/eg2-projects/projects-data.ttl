@base <http://www.example.org/> .
@prefix eg: <http://www.example.org/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix ptp: <http://www.example.org/pythinfer/project/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] owl:imports <pythinfer/project/ontology> .

# ===== EXAMPLE INSTANCES =====

eg:projA a ptp:Project ;
    dct:title "Example Pythinfer Project"@en ;
    dct:description "A sample project demonstrating the ontology structure."@en ;
    dct:creator "Project Creator" ;
    dct:created "2025-11-26"^^xsd:date ;
    ptp:hasInternalData [
        a ptp:DataFile ;
        ptp:inputPattern "data/*.ttl" ;
        dct:title "Project A Data Files"
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/skos.ttl" ;
        dct:title "SKOS Reference Vocabulary" ;
        dct:isVersionOf <http://www.w3.org/2004/02/skos/core>
    ] ;
    ptp:hasExternalData [
        dct:isVersionOf prov:
    ] ;
    .

eg:projB a ptp:Project ;
    dct:title "Example Taxonomy Classification Project"@en ;
    dct:description "A project for managing and classifying taxonomic hierarchies using shared vocabularies."@en ;
    dct:creator "Taxonomy Team" ;
    dct:created "2025-11-20"^^xsd:date ;
    ptp:hasInternalData [
        a ptp:DataFile ;
        ptp:inputPattern "data/*.ttl" ;
        dct:title "Data to Classify"
    ] ;
    ptp:hasInternalData [
        a ptp:DataFile ;
        ptp:inputPattern "taxonomy-data/*.ttl" ;
        dct:title "Taxonomic Classifications"
    ] ;
    ptp:hasExternalData [
        rdfs:label "Official SKOS" ;
        dct:isVersionOf <http://www.w3.org/2004/02/skos/core>
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        dct:title "PROV-O Provenance Ontology"@en ;
        dct:isVersionOf <http://www.w3.org/ns/prov#>
    ] ;
    ptp:hasExternalData [
        dct:title "Friend-of-a-Friend (FOAF)"@en ;
        dct:isVersionOf foaf:
    ] ;
    .

eg:projU a ptp:Project ;
    dct:title "Unrelated Knowledge Graph Project"@en ;
    dct:description "A project using different vocabularies, with insufficient shared sources with other projects to be considered related."@en ;
    dct:creator "Knowledge Graph Team" ;
    dct:created "2025-11-15"^^xsd:date ;
    ptp:basePath "/path/to/knowledge-graph" ;
    ptp:hasInternalData [
        a ptp:DataFile ;
        ptp:inputPattern "kg-data/*.ttl" ;
        dct:title "Knowledge Graph Assertions"
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/schema-org.ttl" ;
        dct:title "Schema.org Vocabulary" ;
        dct:isVersionOf <https://schema.org/>
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        ptp:inputPattern "vocabularies/foaf.ttl" ;
        dct:title "FOAF Vocabulary" ;
        dct:isVersionOf <http://xmlns.com/foaf/0.1/>
    ] ;
    ptp:hasExternalData [
        a ptp:VocabularyFile ;
        dct:title "PROV-O Provenance Ontology"@en ;
        dct:isVersionOf <http://www.w3.org/ns/prov#>
    ] ;
    .
